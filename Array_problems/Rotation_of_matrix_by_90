class Solution {
public:
    void rotate(vector<vector<int>>& matrix) {
        int n=matrix.size();
        for(int j=0;j<n;++j){
            for(int i=j+1;i<n;++i){
                swap(matrix[j][i],matrix[i][j]);
            }
        }
        for(int i =0;i<n;++i){
            reverse(matrix[i].begin(),matrix[i].end());
        }
        
    }
};
